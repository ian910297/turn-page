{
    "name": "Turn Page (comic8)",
    "version": "1.0",
    "description": "Turn Page easily in comic8",
    "background": {
        "scripts": ["background.js"],
        "persistent": false
    },
    "browser_action": {
        "default_popup": "popup.html"
    },
    "permissions": [
        "storage",
        "tabs",
        "http://comicbus.live/*",
        "https://comicbus.live/*"
    ],
    "manifest_version": 2
}